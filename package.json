    "babel-core": "6.26.3",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "express": "4.18.2",
    "img-loader": "4.0.0",
    "morgan": "1.10.0",
    "node-sass": "8.0.0",
